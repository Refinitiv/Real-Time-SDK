			<CompressionType value="CompressionType::None"/>
			<GuaranteedOutputBuffers value="5000"/>
			<Host value="localhost"/>
			<Port value="14003"/>
			<!-- ObjectName is optional: defaulted to ""												-->
			<ObjectName value=""/>
		</Channel>
		
		<!--Performance Tools Channels -->
		<Channel>
			<Name value="Perf_Channel_1"/>
