			<!-- default values -->
			<IProvider>
				<Name value="TestIProv_2"/>
				<Server value="TestServer_2"/>
			</IProvider>
		</IProviderList>
	</IProviderGroup>

	<ChannelGroup>
		<ChannelList>
			<Channel>
