                            <xs:complexType>
                              <xs:attribute name="value" type="xs:unsignedLong" use="required" />
                            </xs:complexType>
                          </xs:element>

                          <xs:element name="DefaultServiceID" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                              <xs:attribute name="value" type="xs:unsignedLong" use="required" />
